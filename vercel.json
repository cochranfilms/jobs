{
  "builds": [
    { "src": "api/**/*.js", "use": "@vercel/node@4" },
    { "src": "*.html", "use": "@vercel/static" },
    { "src": "*.json", "use": "@vercel/static" },
    { "src": "*.png", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/hubspot/contacts", "dest": "/api/hubspot/contacts.js" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}


